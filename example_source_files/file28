int main() {
    int x = 4;
    int y = 5;
    int a = 9;
    int* ptr = &x;
    a = ptr++; // now points to y
    printf("%d", a);
    a = ptr--; // now points to x
    printf("%d", a);

    int is_x = (ptr == &x);
    printf("%d", is_x);
    int is_y = (ptr == &y);
    printf("%d", is_y);
    is_y = (&x != ptr);
    printf("%d", is_y);
    float* ptr2 = 0;

}
